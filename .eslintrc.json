{
    "extends": [
        "eslint:recommended",
        "eslint:import/errors",
        "eslint:react/recomended",
        "eslint:jsx-a11y/recomended",
        "prettier",
       "prettier/react"
    ],
    "rules": {
        "react/prop-types":0,
        "react-hooks/tules-of-hooks":"error",
        "no-console":"warn"
    },
    "parser": "babel-eslint",
    "plugins": ["react","import","jsx-a11y","react-hooks"],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "jest": true
    },
    "settings": {
        "react":{
            "version":"detect"
        }
    }
}